{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.{ts,tsx}",
    "!src/generated/**/*",
    "!src/tools/**/*",
    "!src/**/*.d.ts",
    "!**/__tests__/**/*.ts?(x)",
    "!**/?(*.)+(spec|test|tests).ts?(x)"
  ],
  "testMatch": ["**/__tests__/**/*.ts?(x)", "**/?(*.)+(spec|test|tests).ts?(x)"]
}
