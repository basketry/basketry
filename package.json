{
  "name": "basketry",
  "version": "0.0.9",
  "description": "Generate service-oriented code from popular API definition languages",
  "main": "./lib/index.js",
  "bin": {
    "basketry": "./lib/cli.js"
  },
  "scripts": {
    "test": "jest",
    "clean": "run-s -s clean:*",
    "lint": "run-s -s lint:*",
    "fix": "run-s -s fix:*",
    "clean:output": "rimraf lib",
    "start": "node ./lib/index.js",
    "prebuild": "run-s -s clean lint",
    "build": "tsc",
    "lint:eslint": "eslint src/**/*.*",
    "fix:eslint": "eslint --fix src/**/*.*",
    "lint:prettier": "prettier -c .",
    "fix:prettier": "prettier -w .",
    "clean:coverage": "rimraf coverage",
    "pretest": "run-s -s clean",
    "chmod": "chmod +x ./lib/cli.js",
    "prepack": "run-s build chmod"
  },
  "keywords": [],
  "author": "Steve Konves",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/basketry/basketry.git"
  },
  "bugs": {
    "url": "https://github.com/basketry/basketry/issues"
  },
  "homepage": "https://github.com/basketry/basketry#readme",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^27.4.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "chalk": "^4.1.2",
    "yargs": "^17.4.0"
  }
}
